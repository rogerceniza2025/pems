{
  "name": "pems",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run --parallel dev",
    "build": "pnpm run --recursive build",
    "serve": "pnpm run --parallel serve",
    "test": "pnpm run --recursive test",
    "type-check": "pnpm run --recursive type-check",
    "lint": "pnpm run --recursive lint",
    "lint:styles": "stylelint \"**/*.css\" \"**/*.scss\" \"**/*.less\" --cache",
    "lint:styles:fix": "stylelint \"**/*.css\" \"**/*.scss\" \"**/*.less\" --fix --cache",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix && pnpm run lint:styles:fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:visual": "test-storybook",
    "test:visual:ci": "test-storybook --ci",
    "start": "node .output/server/index.mjs",
    "kill-ports": "powershell -ExecutionPolicy Bypass -File ./kill-ports.ps1"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@kobalte/core": "^0.13.11",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/router-plugin": "^1.139.12",
    "@tanstack/solid-router": "^1.139.12",
    "@tanstack/solid-router-devtools": "^1.139.12",
    "@tanstack/solid-start": "^1.139.12",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-solid": "^0.555.0",
    "nitro": "latest",
    "solid-js": "^1.9.10",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^7.2.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^10.1.2",
    "@storybook/blocks": "^8.6.14",
    "@storybook/html": "^10.1.2",
    "@storybook/html-vite": "^10.1.2",
    "@storybook/test-runner": "^0.24.2",
    "@storybook/testing-library": "^0.2.2",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-config": "^0.3.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-plugin-solid": "^0.14.5",
    "postcss": "^8.5.6",
    "prettier": "^3.7.3",
    "storybook": "^10.1.2",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.12.1",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-solid": "^2.11.10",
    "vitest": "^4.0.14"
  }
}
